generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model User {
  id String @id
  username String
  createdAt DateTime @default(now())
}

model Log {
  id BigInt @id @default(autoincrement())
  type String
  userId String?
  guildId String?
  data Json?
  createdAt DateTime @default(now())

  @@index([userId])
  @@index([guildId])
  @@index([createdAt])
}